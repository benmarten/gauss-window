{
    "extends": "tslint:recommended",
    "rulesDirectory": [],
    "rules": {
        "typedef": [
            true,
            "call-signature"
        ],
        "max-line-length": {
            "options": [
                80
            ]
        },
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": false,
        "semicolon": [
            true,
            "never"
        ],
        "quotemark": [
            true,
            "single"
        ]
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                80
            ]
        }
    }
}